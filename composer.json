{
  "name": "nikazooz/laravel-simplesheet",
  "description": "Simple, powerful and memory-efficient spreadsheet exports and imports in Laravel",
  "license": "MIT",
  "keywords": ["laravel", "php", "box", "spout", "spreadsheet", "excel", "csv", "export", "import", "batch", "simplesheet"],
  "authors": [
    {
      "name": "Nenad Živanović",
      "email": "nen.zivanovic@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.1",
    "box/spout": "^2.7",
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "^3.5",
    "orchestra/database": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Nikazooz\\Simplesheet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nikazooz\\Simplesheet\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nikazooz\\Simplesheet\\SimplesheetServiceProvider"
      ],
      "aliases": {
        "Simplesheet": "Nikazooz\\Simplesheet\\Facades\\Simplesheet"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
